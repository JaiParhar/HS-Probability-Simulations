class CardDeck {
	int cards[] = new int[52];
	
	public CardDeck() {
		for(int i = 0; i < 52; i++) {
			cards[i] = i;
		}
	}
	
	public String getCardFromID(int ID) {
		if(ID < 13) {} else if(ID > 13 && ID < 13*2) {} else if(ID > 13*2 && ID < 13*3) {} else if(ID > 13*3 && ID < 13*4) {}
		
		
		return "";
	}
	
	public void shuffle() {
		
	}
}