		Random randGen = new Random();
		
		for(int i = 0; i < runs; i++) {
			
		}
	}
	
	public static boolean isBetweenInclusive(int i, int bL, int bH) {
		return (bL<=i) && (i<=bH);
	}
	
	public static boolean isBetweenInclusive(double i, double bL, double bH) {
		return (bL<=i) && (i<=bH);
	}

}


