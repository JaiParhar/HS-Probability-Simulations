	public static 
	
	
	//Util functions
	public static boolean isBetweenInclusive(int i, int bL, int bH) { return (bL<=i) && (i<=bH); }
	public static boolean isBetweenInclusiveD(double i, double bL, double bH) {	return (bL<=i) && (i<=bH); }
