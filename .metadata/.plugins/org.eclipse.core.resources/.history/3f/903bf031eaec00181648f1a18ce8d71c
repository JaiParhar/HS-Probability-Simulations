class CardDeck {
	int cards[] = new int[52];
	
	public CardDeck() {
		for(int i = 0; i < 52; i++) {
			cards[i] = i;
		}
		shuffle();
	}
	
	public void shuffle() {
		Random rnd = ThreadLocalRandom.current();
	    for (int i = cards.length - 1; i > 0; i--)
	    {
	      int index = rnd.nextInt(i + 1);
	      int a = cards[index];
	      cards[index] = cards[i];
	      cards[i] = a;
	    }
	}
}