import java.util.Random;
import java.util.concurrent.ThreadLocalRandom;

public class Culminating {
	
	public static void main(String[] args) {
		int runs = 100;
		
		int houseWins = 0;
		int p1Wins = 0;
		int p2Wins = 0;
		
		Random randGen = new Random();
		
		for(int i = 0; i < runs; i++) {
			
		}
	}
	
	public static boolean isBetweenInclusive(int i, int bL, int bH) {
		return (bL<=i) && (i<=bH);
	}
	
	public static boolean isBetweenInclusiveD(double i, double bL, double bH) {
		return (bL<=i) && (i<=bH);
	}

}


class CardDeck {
	int cards[] = new int[52];
	
	public CardDeck() {
		for(int i = 0; i < 52; i++) {
			cards[i] = i;
		}
	}
	
	public void shuffle() {
		Random rnd = ThreadLocalRandom.current();
	    for (int i = ar.length - 1; i > 0; i--)
	    {
	      int index = rnd.nextInt(i + 1);
	      // Simple swap
	      int a = ar[index];
	      ar[index] = ar[i];
	      ar[i] = a;
	    }
	}
}